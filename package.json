{
  "name": "twiggle-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:query": "dotenv -e .env.local -- tsx scripts/query-db.ts",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.0",
    "@google-cloud/storage": "^7.17.3",
    "@prisma/client": "^5.19.0",
    "@xyflow/react": "^12.9.3",
    "lucide-react": "^0.554.0",
    "next": "16.0.4",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.10.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^9.0.8",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "prisma": "^5.19.0",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
